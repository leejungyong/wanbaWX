page{
 background-color: #121a21;
}
.flex {
  
  width: 720rpx;
  margin: 30rpx 15rpx 0 15rpx;
  text-align: center;
  display: flex;
  flex-wrap: wrap;  
  justify-content : flex-start;
  
}
.flex .box {

  display: table;
  height:56px;
  width: 28px;
  float: left;
  vertical-align: middle;
  text-align: center;
  padding-right:5px;
}
.flex .box .icon {
  
  height: 28px;
  width: 28px;
  float: right;
}
.flex .box .txt {
  font-size: 10pt;
  display: table-cell;
  line-height: 16px;
  height: 16px;
  width: 36px;
  float: left;
  padding:0;
  margin-top:2px;
  color:#946b25;
}

.flex .box .share {
  background-image: url('data:image/png;base64,');
  border: none;
  height: 28px;
  width: 28px;
  padding-right: 0px;
  margin:1px 5px 0 0;
  
  /* display: inine-block; */
  float: right;
}
