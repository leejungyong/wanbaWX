.table {
  margin-top: 60px;
}

.table .item {
  width: 90%;
  height: 80px;
  margin: 10px 5% 0 5%;
  border: 1px solid;
  border-image: linear-gradient(90deg,#121a21 0, #24435d 25%, #1a93d0 50%, #24435d 75%, #121a21 100%) 30 0 30 0;
}

.left_div {
  width: 30%;
  height: 80px;
}

.img_div {
  width: 30%;
  height: 80px;
  float: left;
  
}

.img_div .img {
  padding: 10px;
  height: 64px;
  width: 64px;
  
}

.right_div {
  width: 70%;
  height: 70px;
  float: right;
  display: table;
  
  padding: 10px 0 5px 0;
}

.div1 {
  width: 100%;
