page {
  background: #121a21;
}

.modal-box {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0px;
  background: rgba(0, 0, 0, 0.4);
  overflow: hidden;
  z-index: 990;
}

.modal-body {
  position: relative;
}

.modal-content {
  /* border:1px solid #c9c9c9; */
  border: 0;
  width: 650rpx;
  height: 360rpx;
  margin: 40% 50rpx 0 50rpx;
  overflow: hidden;
  /* border-radius: 10rpx; */
  z-index: 999;
  background-size: cover;
  background-repeat: no-repeat;
  background-image: url('data:image/jpeg;base64,');
}

.modal-btn {
  text-align: center;
}

.input {
  border: 1px solid #24435d;
  background-color: transparent;
  margin: 50px;
  height: 30rpx;
  color: #1989c3;
}

.input-placeholder {
  color: #24435d;
}

.cancel_btn {
  height: 60rpx;
  width: 228rpx;
}

.question {
  width: 90%;
  margin: 20px 5% 0 5%;
  background-color: transparent;
  color:#1989c3;
  /* border:1px solid #1989c3; */
}
.gold{
  color:#946b25;
}
