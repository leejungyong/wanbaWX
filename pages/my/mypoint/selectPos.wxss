page {
  background: #121a21;
  color: #00a0e9;
}

.top {
  width: 100%;
  height: 10vh;
  background: #0c1319;
}

.top .address {
  height: 10vh;
  width: 500rpx;
  float: left;
  line-height: 10vh;
  vertical-align: center;
  font-size: 14px;
  display: inline;
  padding-left: 20rpx;
}

.top .confirm {
  height: 10vh;
  width: 200rpx;
  float: right;
  display: inline;
}

.top .confirm button {
  margin-right: 30rpx;
  margin-top: 30rpx;
  height: 60rpx;
  line-height: 57rpx;
  width: 150rpx;
  border: 1px solid #00a0e9;
  text-align: center;
  color: #00a0e9;
  background: #1c3043;
  font-size: 14px;
}

.latlng {
  width: 100%;
  height: 8vh;
}

.latlng .location {
  height: 8vh;
  width: 500rpx;
  float: left;
  line-height: 8vh;
  vertical-align: center;
  font-size: 14px;
  display: inline;
  padding-left: 20rpx;
}

.latlng .import {
  height: 8vh;
  width: 200rpx;
  float: right;
  display: inline-block;
  font-size: 14px;
  line-height: 8vh;
  vertical-align: center;
}
